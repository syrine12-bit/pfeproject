                <script>
                    // Récupération des éléments DOM
                    const addBtn = document.querySelector('.add-btn');
                    const deleteBtns = document.querySelectorAll('.delete-btn');
                    const container = document.querySelector('#field-container');

                    // Ajout d'un événement de clic sur le bouton "Ajouter"
                    addBtn.addEventListener('click', () => {
                        // Création d'un nouvel élément de champ
                        const newField = document.createElement('div');
                        newField.classList.add('row', 'mb-3');
                        newField.innerHTML = `
                          <div class="mb-3">
                           <input type="text" name="formation" class="form-control mb-3" placeholder="Formation" required>
                           </div>
                          <div class="row">
                        <div class="col"><input type="text" name="etablissement" class="form-control mb-3" placeholder="Établissement" required></div>
                         <div class="col">
                         <input type="text" name="lieu" class="form-control mb-3" placeholder="ville" required></div>
                        </div>
                      <div class="mb-3">
                       <input type="text" name="description" class="form-control mb-3" placeholder="Description" required></div>
             <div class="mb-3 mt-3 text-end">
                  <button class="btn btn-danger delete-btn">Supprimer</button>
                </div>

              `;
                        // Insertion du nouveau champ dans le conteneur
                        container.appendChild(newField);
                        // Ajout d'un événement de clic sur le bouton "Supprimer" du nouveau champ
                        const newDeleteBtn = newField.querySelector('.delete-btn');
                        newDeleteBtn.addEventListener('click', () => {
                            // Suppression du champ correspondant
                            newField.remove();
                        });
                    });

                    // Ajout d'un événement de clic sur chaque bouton "Supprimer" déjà présent
                    deleteBtns.forEach(deleteBtn => {
                        deleteBtn.addEventListener('click', () => {
                            // Suppression du champ correspondant
                            deleteBtn.parentNode.parentNode.remove();
                        });
                    });


                </script>
